RewriteEngine on
#RewriteRule ^.*$ http://www.google.com [NC,L,R=302]

#Map page
#allow for generic params...
RewriteRule ^sap/#location=(1?[0-9]?)/([-+]?\d{1,2}[.]\d+)/([-+]?\d{1,3}[.]\d+){1}((&[a-zA-Z0-9]+=[a-zA-Z0-9\./]+)*)[/]?$ map.php?zoom=$1&lat=$2&lon=$3$4 [NC,L]
RewriteRule ^map/#location=(1?[0-9]?)/([-+]?\d{1,2}[.]\d+)/([-+]?\d{1,3}[.]\d+){1}((&[a-zA-Z0-9]+=[a-zA-Z0-9\./]+)*)[/]?$ map.php?zoom=$1&lat=$2&lon=$3$4 [NC,L]
#location only...
#RewriteRule ^nap/#location=(1?[0-9]?)/([-+]?\d{1,2}[.]\d+)/([-+]?\d{1,3}[.]\d+)(/)?$ map.php?zoom=$1&lat=$2&lon=$3 [NC,L]
#RewriteRule ^map/#location=(1?[0-9]?)/([-+]?\d{1,2}[.]\d+)/([-+]?\d{1,3}[.]\d+)(/)?$ map.php?zoom=$1&lat=$2&lon=$3 [NC,L]
# no params...
RewriteRule ^sap(/)?$ map.php [NC,L]
RewriteRule ^map(/)?$ map.php [NC,L]

#Static pages
RewriteRule ^about$ about.php
RewriteRule ^data$ data.php